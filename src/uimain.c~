int main() {
  printf("Welcome to Isabella's Tokenizer! Please enter a string, type 'h' to see the history, or 'q' to quit!");
  char str[] = "  Hello World ";
  char *token = token_start(str);

  if (token != NULL) {
    printf("First token: %s\n", token);
  } else {
    printf("No tokens found.\n");
  }
    
    return 0;
}

